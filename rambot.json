   "init": "upstart"
 },
 "nodejs": {
   "install_method": "package"
 },
 "hubot": {
   "version": "v2.5.0",
   "dependencies": {
     "twit": "latest",
     "hubot-irc": "0.1.12",
     "underscore": "latest"
   },
   "env": {
     "HUBOT_IRC_NICK": "rambot",
